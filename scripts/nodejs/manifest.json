{
    "id": "nodejs",
    "name": "Node.js (LTS)",
    "description": "JavaScript runtime built on Chrome's V8 engine.",
    "check": {
        "command": "node --version",
        "type": "cmd"
    },
    "install": {
        "command": "install.bat"
    },
    "uninstall": {
        "command": "winget uninstall OpenJS.NodeJS.LTS"
    },
    "locate": {
        "command": "where node"
    }
}