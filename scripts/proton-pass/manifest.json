{
    "id": "proton-pass",
    "name": "Proton Pass",
    "description": "Secure password manager with end-to-end encryption.",
    "check": {
        "command": "cmd /c \"if exist \"%LOCALAPPDATA%\\ProtonPass\\ProtonPass.exe\" (exit 0) else (exit 1)\"",
        "type": "cmd"
    },
    "install": {
        "command": "install.bat"
    },
    "uninstall": {
        "command": "winget uninstall Proton.Pass"
    },
    "locate": {
        "command": "echo %LOCALAPPDATA%\\ProtonPass\\ProtonPass.exe"
    }
}