{
    "id": "proton-vpn",
    "name": "Proton VPN",
    "description": "High-speed Swiss VPN that safeguards your privacy.",
    "check": {
        "command": "cmd /c \"if exist \"C:\\Program Files\\Proton\\VPN\\ProtonVPN.exe\" (exit 0) else (exit 1)\"",
        "type": "cmd"
    },
    "install": {
        "command": "install.bat"
    },
    "installOptions": [
        {
            "label": "GitHub Release (Fast)",
            "source": "github",
            "icon": "github",
            "command": "install_github.bat"
        },
        {
            "label": "Official Site (Latest)",
            "source": "official",
            "icon": "globe",
            "command": "install_official.bat"
        }
    ],
    "uninstall": {
        "command": "winget uninstall Proton.VPN"
    },
    "locate": {
        "command": "echo C:\\Program Files\\Proton\\VPN\\ProtonVPN.exe"
    }
}